# Copy this to .env.local

# Qdrant Cloud
QDRANT_URL=
QDRANT_API_KEY=
QDRANT_COLLECTION=docs_text-embedding-004

# Embeddings
EMBEDDINGS_MODEL=text-embedding-004
GOOGLE_API_KEY=

# LLM
LLM_PROVIDER=gemini # gemini | deepseek
LLM_MODEL=gemini-1.5-flash
# Optional: use a different LLM only for chart extraction
# e.g., set provider to gemini and model to a stronger variant when plotting is needed
LLM_PROVIDER_CHART=
LLM_MODEL_CHART=
DEEPSEEK_API_KEY=
